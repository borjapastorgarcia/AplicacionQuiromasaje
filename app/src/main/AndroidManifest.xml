<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.borja.aplicacionquiromasaje" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activityRegistro"
            android:label="@string/title_activity_activity_registro"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".activityAdministrador"
            android:label="@string/title_activity_activity_administrador"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".ActivityUsuario"
            android:label="@string/title_activity_activity_usuario"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
